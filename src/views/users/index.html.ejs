<div class="base bgcolor-0 round-1 container">
<h2>User index</h2>
  <table>
    <thead>
      <tr>

        <th>id</th>
        <th>Username</th>
        <th>Name</th>
        <th>email</th>
        <th>address</th>
        <th colspan="2">Password</th>
      </tr>
    </thead>
    <tbody>
      <% usersList.forEach((user) => { %>
        <tr>
       	  <td><%= user.id %></td>
          <td><%= user.username %></td>
          <td><%= user.name %></td>
          <td><%= user.email %> </td>
          <td><%= user.address %> </td>
          <td><%= user.password %> </td>
        </tr>

      <% }) %>
    </tbody>
  </table>


</div>
